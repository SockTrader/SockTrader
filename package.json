{
  "name": "socktrader",
  "version": "0.0.0-development",
  "description": "Websocket based trading bot for cryptocurrencies",
  "repository": "https://github.com/SockTrader/SockTrader",
  "license": "GPL-3.0",
  "keywords": [
    "tradingbot",
    "algorithmic-trading",
    "cryptocurrencies",
    "binance",
    "trading-algorithms",
    "trading-bot",
    "trading",
    "crypto",
    "bitcoin"
  ],
  "workspaces": [
    "packages/**",
    "apps/**"
  ],
  "scripts": {
    "nx": "nx",
    "cli": "cd packages/cli && ts-node -r tsconfig-paths/register src/index.ts",
    "clean": "nx reset; npx rimraf dist node_modules **/node_modules",
    "build:all": "nx run-many --target=build --all",
    "build:affected": "nx affected:build",
    "lint:all": "nx run-many --target=lint --all",
    "lint:affected": "nx affected:lint",
    "test:all": "cross-env TZ=UTC nx run-many --target=test --verbose --all --parallel",
    "test:affected": "cross-env TZ=UTC nx affected:test --verbose",
    "start:backtest": "nx serve demo",
    "start:web": "nx serve web"
  },
  "dependencies": {
    "@ngneat/elf": "^2.1.0",
    "@ngneat/elf-entities": "^4.3.1",
    "binance-api-node": "^0.11.40",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "nanoid": "^3.3.4",
    "nconf": "^0.12.0",
    "pg": "^8.7.3",
    "rxjs": "7.5.5",
    "technicalindicators": "^3.1.0",
    "tslib": "^2.4.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@nrwl/cli": "^14.3.6",
    "@nrwl/eslint-plugin-nx": "^14.3.6",
    "@nrwl/jest": "^14.3.6",
    "@nrwl/js": "^14.3.6",
    "@nrwl/linter": "^14.3.6",
    "@nrwl/node": "^14.3.6",
    "@nrwl/workspace": "^14.3.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/config": "0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/nconf": "^0.10.2",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "conventional-changelog-eslint": "^3.0.9",
    "cross-env": "^7.0.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "jest": "28.1.1",
    "nx": "14.3.6",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.4"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "authors": [
    "wouter cleymans",
    "thijs raets"
  ]
}
